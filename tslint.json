{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": false,
        "ter-indent": false,
        "object-shorthand-properties-first": false,
        "align": false,
        "no-increment-decrement": false,
        "semicolon": [
            true,
           "always",
           "ignore-interfaces"
        ]
    },
    "rulesDirectory": []
}
